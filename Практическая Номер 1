import math


def lab():
    global y
    try:
        a = int(input("Введите а ="))
        b = int(input("Введите b = "))
        x = int(input("Введите x = "))
        if x >= 4:
            y = (a ** 2 + 5 * x + b ** 2) / (a * b)
        else:
            y = x * (a - b)

    except:
        print("Данные введны неверно либо расчет невозможен")
        input()

    print("y = %.1f" % y)


if __name__ == "__main__":
    lab()
    input()
